{
    "name": "salmanzafar/laravel-geocode",
    "description": "A Laravel package that makes it to find lat and long from a specific address",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Salman Zafar",
            "email": "salmanzafar949@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "laravel5",
        "laravel6",
        "laravel7",
        "geocode-laravel",
        "laravel-geocode",
        "google maps laravel geocode",
        "google maps laravel geocoder",
        "php",
        "php 7.0",
        "php 7.1",
        "php 7.2",
        "php 7.3",
        "php 7.4"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.5"
    },
    "require-dev": {
        "ext-curl": "*"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Salman\\GeoCode\\": "src/"
        },
        "files": [
            "src/Helper/Helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Salman\\GeoCode\\GeoCodeServiceProvider"
            ],
            "aliases": {
                "GeoCode": "Salman\\GeoCode\\Facades\\GeoCode"
            }
        }
    }
}
